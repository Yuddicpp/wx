.title{
  width: 100%;
  height: 30px;
  line-height: 30px;
  text-align: center;
  color: #fff;
  background-color: #FFB90F;
}
.reback{
  display: inline-block;
  position: absolute;
  left: 0px;
  color: #fff;
  background-color: #FFB90F;
  text-align: center;
}
.titles{
  color: #FFB90F;
  margin: 7px;
  font-family:'Franklin Gothic Medium', 'Arial Narrow', Arial, sans-serif;
}
.input_box{
  border: solid 1px #FFF68F;
  color: 	#2F4F4F;
  height: 30px;
  margin: 0px;
  padding: 0%;
  width: 100%;
}
.picker{
  border: 1px solid #FFF68F;
  padding-left: 30px;
  color: 	#2F4F4F;
  height: 30px;
}
.area{
  border: solid 1px #FFF68F;
  height: 120px;
  width: 100%;  
  color: 	#2F4F4F;
}
.ending{
  position: absolute;
  left: 0;
  right: 0;
  border-radius: 50%;
  color: #fff;
  background-color: #FFB90F;
  width: 80px;
  height: 80px;
  line-height: 80px;
}