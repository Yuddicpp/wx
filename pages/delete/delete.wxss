.information {
    padding-left: 10px;
    color: #ffb90f;
    font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  }
  
  .starting_point {
    position: relative;
    height: 30px;
    width: 100%;
  }
  
  .starting_point_photo {
    width: 30px;
    height: 30px;
    background-size: 100%;
    margin: 0px;
    display: inline-block;
    position: absolute;
  }
  
  .starting_point_text {
    position: absolute;
    display: inline-block;
    text-align: center;
    color: #ffb90f;
    padding-left: 40px;
  }
  
  .end {
    position: relative;
